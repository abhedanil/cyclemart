    <script>

      function removeProductfromwishlist(proId) {
					Swal.fire({
						title: 'Are you sure?',
						text: "You won't be able to revert this!",
						icon: 'warning',
						showCancelButton: true,
						confirmButtonColor: '#3085d6',
						cancelButtonColor: '#d33',
						confirmButtonText: 'Yes, delete it!'
					}).then((result) => {
						if (result.isConfirmed) {
								removeProductfromwish()		
						}
					})
		function removeProductfromwish() {
			alert(proId)
         $.ajax({
            url: "/deletprofromwishlist",
            data: {
              proId: proId,
            },
            method: "post",
            success: (Response) => {
              if (Response) {
                location.reload()
              }
            }
          })     
				}
		}



    </script>